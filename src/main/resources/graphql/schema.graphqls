scalar Long
scalar Timestamp
scalar Aggregation

type CountComponent {
    name: String
    value: Int
}

type Count {
    timestamp: Timestamp!
    components: [CountComponent!]!
}

type CountQueries {
    latest: Count
    interval(from: Timestamp!, to: Timestamp!, type: Aggregation = "AVG", points: Int = 100): [Count]
}

schema {
    query: CountQueries
}
